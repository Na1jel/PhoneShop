<nav>
    <ul class="pagination">
        {{#paginate pagination type="first"}}
            <li class="page-item {{#if disabled}}disabled{{/if}}" {{#if disabled}} tabindex="-1" aria-disabled="true" {{/if}}>
                <a class="page-link" href="?page={{n}}">First</a>
            </li>
        {{/paginate}}
        {{#paginate pagination type="previous"}}
            <li class="page-item {{#if disabled}}disabled{{/if}}" {{#if disabled}} tabindex="-1" aria-disabled="true"{{/if}}>
                <a class="page-link" href="?page={{n}}">Prev</a>
            </li>
        {{/paginate}}
        {{#paginate pagination type="middle" limit="7"}}
            <li class="page-item {{#if active}}active{{/if}} {{#if disabled}}disabled{{/if}}" >
                <a class="page-link" href="?page={{n}}">{{n}}</a>
            </li>
        {{/paginate}}
        {{#paginate pagination type="next"}}
            <li class="page-item {{#if disabled}}disabled{{/if}}" {{#if disabled}} tabindex="-1" aria-disabled="true"{{/if}}>
                <a class="page-link" href="?page={{n}}">Next</a>
            </li>
        {{/paginate}}
        {{#paginate pagination type="last"}}
            <li class="page-item {{#if disabled}}disabled{{/if}}" {{#if disabled}} tabindex="-1" aria-disabled="true"{{/if}}>
                <a class="page-link" href="?page={{n}}">Last</a>
            </li>
        {{/paginate}}
    </ul>
</nav>